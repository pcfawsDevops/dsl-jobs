job('dsl_child3')
{
  description('second child job from SEED')
  parameters 
  {
    stringParam('Planet', defaultValue = 'world', description = 'This is world')
    booleanParam('Flag', true)
    choiceParam('Option', [' option1 (default) ','option2','option3'])
  }
  scm 
  {
    git( 'https://github.com/pcfawsDevops/HelloWorld.git', 'master' )
  }
  
  triggers 
  {
    cron('* * * * *')
  }
  
  steps 

  {
    shell(""" 
	echo 'Hello world'
	echo 'from Git hook'
	pwd
	 """)
  }
}

